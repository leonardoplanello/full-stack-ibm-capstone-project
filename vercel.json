{
  "version": 2,
  "builds": [
    {
      "src": "server/api/index.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/server/staticfiles/$1"
    },
    {
      "src": "/media/(.*)",
      "dest": "/server/staticfiles/media/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/server/api/index.py"
    }
  ],
  "env": {
    "PYTHONPATH": "server",
    "VERCEL": "1",
    "DEBUG": "False"
  }
}

